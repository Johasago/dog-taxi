{
    "name": "dog-taxi",
    "version": "1.0.0",
    "description": "An app to get dogs from A to B",
    "main": "main.js",
    "scripts": {
        "client-install": "npm install --prefix client",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client        && npm run build --prefix client"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Johasago/dog-taxi.git"
    },
    "author": "Joanna Taylor",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Johasago/dog-taxi/issues"
    },
    "homepage": "https://github.com/Johasago/dog-taxi#readme",
    "engines": {
        "node": "10.11.0"
    },
    "dependencies": {
        "@ideal-postcodes/core-node": "^1.1.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "concurrently": "^5.0.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-fileupload": "^1.1.6-alpha.5",
        "mongodb": "^3.3.2",
        "mongoose": "^5.7.5",
        "mongoose-unique-validator": "^2.0.3",
        "multer": "^1.4.2",
        "node-postcodes.io": "^1.0.0",
        "pug": "^2.0.4"
    }
}
